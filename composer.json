{
    "name": "benjivm/merge-pdf",
    "description": "Library for merging multiple PDFs",
    "keywords": ["pdf", "merge"],
    "homepage": "https://github.com/benjivm/merge-pdf",
    "type": "library",
    "license": "WTFPL",
    "authors": [
        {
            "name": "Hannes Forsg√•rd",
            "email": "hannes.forsgard@fripost.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "Benjivm\\MergePdf\\": "src/"
        },
        "classmap": [
            "tcpdi/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1||^8.0",
        "tecnickcom/tcpdf": "^6.2.22",
        "setasign/fpdi": "^2"
    },
    "conflict": {
        "setasign/fpdf": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "smalot/pdfparser": "~0.13",
        "phpspec/prophecy": "^1.19",
        "friendsofphp/php-cs-fixer": "^3.9",
        "symfony/var-dumper": "^7.1"
    }
}
